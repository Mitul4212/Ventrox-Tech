{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/test",
            "destination": "/api/test.ts"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "outputDirectory": "dist/public"
}